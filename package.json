    "moment": "^2.22.1",
    "readdirp": "^2.1.0",
    "reflect-metadata": "^0.1.10",
    "request": "^2.87.0",
    "rimraf": "~2.5.4",
    "seedrandom": "^2.4.3",
    "winston": "~2.3.1"
  },
  "devDependencies": {
    "@types/chai": "*",
