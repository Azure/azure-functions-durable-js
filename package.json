    "@types/debug": "0.0.29",
    "@types/mocha": "*",
    "@types/node": "^6.0.31",
    "@types/rimraf": "0.0.28",
    "@types/uuid": "^3.4.4",
    "@types/winston": "~2.2.0",
    "chai": "*",
    "mocha": "*",
